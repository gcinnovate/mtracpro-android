<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eq62roket.mtracpro">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AptActivity"
            android:label="APT."
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".MatActivity"
            android:label="MAT."
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".ArvActivity"
            android:label="ARV."
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".TraActivity"
            android:label="TRA."
            android:parentActivityName=".MainActivity" />
        <activity android:name=".DeathActivity"
            android:label="DEATH."
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".CasesActivity"
            android:label="CASES."
            android:parentActivityName=".MainActivity"></activity>
    </application>

</manifest>