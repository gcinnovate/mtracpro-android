<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.eq62roket.mtracpro.MainActivity"
    tools:showIn="@layout/app_bar_main">

    <ImageView
        android:id="@+id/imageCases"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="44dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="31dp"
        app:srcCompat="@drawable/cases"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:id="@+id/imageDeath"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="23dp"
        android:layout_marginStart="23dp"
        app:srcCompat="@drawable/death"
        android:layout_alignTop="@+id/imageCases"
        android:layout_toRightOf="@+id/imageCases"
        android:layout_toEndOf="@+id/imageCases" />

    <ImageView
        android:id="@+id/imageApt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        app:srcCompat="@drawable/apt"
        android:layout_below="@+id/imageCases"
        android:layout_alignLeft="@+id/imageCases"
        android:layout_alignStart="@+id/imageCases" />

    <ImageView
        android:id="@+id/imageMat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/mat"
        android:layout_alignTop="@+id/imageApt"
        android:layout_alignLeft="@+id/imageDeath"
        android:layout_alignStart="@+id/imageDeath" />

    <ImageView
        android:id="@+id/imageTra"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/tra"
        android:layout_below="@+id/imageApt"
        android:layout_alignLeft="@+id/imageApt"
        android:layout_alignStart="@+id/imageApt"
        android:layout_marginTop="31dp" />

    <ImageView
        android:id="@+id/imageArv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageMat"
        android:layout_alignStart="@+id/imageMat"
        android:layout_alignTop="@+id/imageTra"
        app:srcCompat="@drawable/arv" />
</RelativeLayout>
